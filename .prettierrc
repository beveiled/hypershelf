{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "trailingComma": "all",
  "endOfLine": "lf",
  "semi": true,
  "singleQuote": false,
  "quoteProps": "as-needed",
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "plugins": [
    "prettier-plugin-tailwindcss",
    "@trivago/prettier-plugin-sort-imports"
  ],
  "importOrder": [
    "^@/components/(.*)$",
    "^@/convex/(.*)$",
    "^@/lib/(.*)$",
    "^@/(.*)$",
    "^[./]",
    "^.*$"
  ],
  "importOrderSortSpecifiers": true
}
